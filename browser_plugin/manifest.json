{
  "name": "GTA Online Traxxx",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Collect informations from a GTA race and store it on traxxx.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
/*      "run_at": "document_end",
      "all_frames": true, */
      "js": ["apply.js"]
    }
  ],
  "permissions": [
    "tabs",
    "https://de.socialclub.rockstargames.com/",
    "https://socialclub.rockstargames.com/"
    ]
}

